<?xml version="1.0" encoding="UTF-8"?>
<resources>
	<color name="theme_primary_90">#E6262626</color>
	<style name="Theme.Nearest" parent="@style/Theme.AppCompat.Light">
		<!-- Your customizations go here -->
		<item name="android:windowFullscreen">false</item>
		<item name="android:windowContentOverlay">@null</item>
		<item name="android:statusBarColor">@color/app_bg</item>
		<item name="android:navigationBarColor">@color/app_bars</item>
		<item name="android:textColorPrimary">@color/textblack</item>
		<item name="android:colorPrimary">@color/app_bar</item>
		<item name="android:colorPrimaryDark">@color/app_bar</item>
		<item name="android:windowBackground">@color/app_bg</item>
		<item name="android:windowLightStatusBar">true</item>
		<item name="android:windowDrawsSystemBarBackgrounds">true</item>
	</style>
	<style name="InfoTheme" parent="Theme.Nearest"> 
		<item name="windowActionModeOverlay">true</item>
		<item name="android:windowBackground">@color/app_bg_alt</item>
		<item name="android:textColorPrimary">@color/white</item>
		<item name="android:textColorSecondary">@color/lightgray</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:windowContentOverlay">@null</item>
	</style>
</resources>